    "import numpy as np\n",
    "\n",
    "def to_categorical(x, n_col=None):\n",
    "    array_result= []\n",
    "    classes=max(x)+1\n",
    "    arrayi = []\n",
    "    for i in x:\n",
    "        arrayi=[0]*classes\n",
    "        arrayi[i]=1\n",
    "        array_result.append(arrayi)\n",
    "    return array_result\n",
    "\n",
    "A = [2,3,4]\n",
    "print(to_categorical(A))"
   ]
